{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "esnext",
    "target": "ES5",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["es2020", "DOM", "DOM.Iterable"],
    "baseUrl": "./src",
    "keyofStringsOnly": true
  },
  "exclude": ["node_modules", "dist", "src"]
}
